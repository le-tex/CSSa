        <data type="token">
          <param name="pattern">scaleY\s*\(\s*([\-\+]?\.[0-9]+|[0-9]+\.?([0-9]+)?)\s*\)</param>
        </data>
        <data type="token">
          <param name="pattern">scale\s*\(\s*([\-\+]?\.[0-9]+|[0-9]+\.?([0-9]+)?)(\s*,\s*([\-\+]?\.[0-9]+|[0-9]+\.?([0-9]+)?))?\s*\)</param>
        </data>
      </choice>
    </define>

